<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.dionysource.motg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-sdk android:minSdkVersion="19" android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MOTG"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.nhn.android.naverlogin.ui.OAuthLoginActivity"
            android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:name="com.nhn.android.naverlogin.ui.OAuthLoginInAppBrowserActivity"
            android:label="OAuth2.0 In-app"/>
        <activity android:name=".auth.naverLogin" />
        <activity android:name=".auth.naverMaster" />
        <activity android:name=".auth.Base" />
        <activity android:name=".auth.facebookMaster" />
        <activity android:name=".auth.googleMaster" />
        <activity android:name=".auth.kakaoMaster" />
        <activity android:name=".init"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.Manager" />
    </application>

</manifest>