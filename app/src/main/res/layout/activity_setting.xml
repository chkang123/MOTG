<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kangchanghoon.oop.SettingActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/intro"
        android:text="자신의 평가기준 1~5위를 번호로 입력해주세요!"
        android:textAlignment="center"
        android:textSize="15sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/intro2"
        android:text="중요하지 않은 항목이라 생각하면 0을 넣어주세요!"
        android:textAlignment="center"
        android:layout_below="@+id/intro"
        android:textSize="15sp" />

    <TextView
        android:text="기준"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:id="@+id/intro3"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="80dp" />

    <TextView
        android:text="거리"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:id="@+id/textView13"
        android:textSize="24sp"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="80dp" />

    <TextView
        android:text="비용"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="190dp"
        android:id="@+id/textView14"
        android:textSize="24sp"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="80dp" />

    <TextView
        android:text="서비스"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="290dp"
        android:id="@+id/textView16"
        android:textSize="24sp"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="70dp" />

    <TextView
        android:text="타인의 평점"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:id="@+id/textView15"
        android:textSize="24sp"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="40dp" />

    <TextView
        android:text="맛"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:id="@+id/textView12"
        android:textSize="24sp"
        android:layout_below="@+id/intro2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="90dp" />

    <TextView
        android:text="순위"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/intro4"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:layout_below="@+id/intro2"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="80dp"
        android:layout_marginTop="40dp" />

    <EditText
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText1"
        android:layout_height="wrap_content"
        android:layout_width="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="123dp" />

    <EditText
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText5"
        android:layout_height="wrap_content"
        android:layout_width="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="323dp" />

    <EditText
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_height="wrap_content"
        android:layout_width="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="273dp" />

    <EditText
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_height="wrap_content"
        android:layout_width="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="223dp" />

    <EditText
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_height="wrap_content"
        android:layout_width="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="173dp" />

    <Button
        android:text="등록하기"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="26dp"
        android:id="@+id/button_set" />

</RelativeLayout>
